{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-shadow": [
      "error",
      {
        "builtinGlobals": true,
        "hoist": "all",
        "allow": []
      }
    ]
  }
}
